import psi4
import numpy as np


mol = psi4.geometry("""
0 1
O
H 1 1.0
H 1 1.0 2 104.5
""")

psi4.core.set_global_option("BASIS", "6-31G")
psi4.core.set_output_file("output.dat", False)

print np.array(psi4.energy("B3LYP", molecule=mol))

mol = psi4.geometry("""
1 2
O
H 1 1.0
H 1 1.0 2 104.5
""")

psi4.core.set_global_option("REFERENCE", "UKS")

print np.array(psi4.energy("B3LYP", molecule=mol))

